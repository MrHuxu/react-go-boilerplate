{
  "name": "react-go-boilerplate",
  "version": "0.0.1",
  "description": "",
  "main": "main.go",
  "scripts": {
    "lint": "./node_modules/eslint/bin/eslint.js . --fix --ignore-path .gitignore --ext .jsx --ext .js; exit 0",
    "lint-watch": "./node_modules/eslint-watch/bin/esw . --fix --ignore-path .gitignore --ext .jsx --ext .js -w",
    "webpack-dev-server": "./node_modules/webpack-dev-server/bin/webpack-dev-server.js --config webpack.config.dev.js --port 6789 --hot",
    "dev-server": "gin --port 8283 --appPort 13109 run main.go",
    "build-frontend": "./node_modules/webpack/bin/webpack.js --config webpack.config.prd.js",
    "build-backend": "go build main.go",
    "build": "npm run build-frontend && npm run build-backend",
    "dev": "node ./scripts/dev-server",
    "prd": "npm run build && GIN_MODE=release ./main"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MrHuxu/react-go-boilerplate.git"
  },
  "keywords": [
    "react",
    "go",
    "boilerplate"
  ],
  "author": "MrHuxu hxtheone@gmail.com",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/MrHuxu/react-go-boilerplate/issues"
  },
  "homepage": "https://github.com/MrHuxu/react-go-boilerplate#readme",
  "dependencies": {
    "antd": "^3.2.1",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-dev-expression": "^0.2.1",
    "babel-plugin-import": "^1.6.5",
    "babel-plugin-syntax-decorators": "^6.13.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-export-extensions": "^6.22.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "css-loader": "^0.28.8",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^1.1.5",
    "history": "^4.7.2",
    "jquery": "^3.2.1",
    "less": "^3.0.1",
    "less-loader": "^4.0.5",
    "nprogress": "^0.2.0",
    "prop-types": "^15.6.0",
    "radium": "^0.22.0",
    "react": "^16.1.1",
    "react-dom": "^16.1.1",
    "react-redux": "^5.0.7",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "react-router-redux": "^5.0.0-alpha.6",
    "redux": "^3.7.2",
    "redux-thunk": "^2.2.0",
    "style-loader": "^0.20.2",
    "url-loader": "^0.6.2",
    "webpack": "^3.8.1",
    "webpack-dev-server": "^2.10.1"
